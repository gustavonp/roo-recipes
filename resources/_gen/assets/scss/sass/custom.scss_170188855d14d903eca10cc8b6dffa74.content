:root{--white:#ffffff;--black:#000000;--cream:#ffedd9;--espresso:#352923;--cappuccino:#884615;--mocha:#7d3703;--grey:#4b4b4b;--orange:#f16c07;--normal-font:'Wotfard';--titles-font:'Litila One';--first-breakpoint:1000px;--second-breakpoint:700px;--margin-full:144px;--margin-half:50px;--margin-small:16px}@font-face{font-family:wotfard;font-style:normal;font-weight:100 900;font-display:block;src:url(/fonts/Wotfard-Regular.otf)format("opentype")}@font-face{font-family:litila one;font-style:normal;font-weight:100 900;font-display:block;src:url(/fonts/LilitaOne-Regular.ttf)format("truetype")}@media screen and (max-width:768px){.logo-wrapper{text-align:center}.logo-wrapper .logo{margin-bottom:50px}.roo{display:none}.logo-nav-wrapper{flex-direction:column-reverse!important}header .nav-wrapper{margin-top:0!important}.logo-nav-wrapper{flex-direction:column!important}.nav-logo{flex-direction:column-reverse!important}.my-flexbox.row-collapse{flex-direction:column!important}.post-container{margin-top:15px}}@media(min-width:768px){.container{width:750px}.roo{width:355px}}@media(min-width:992px){.container{width:970px}.roo{width:355px}}@media(min-width:1200px){.container{width:1171px}.roo{width:355px}}#tooltip{cursor:help;text-decoration:underline;color:var(--orange)}body{font-family:var(--normal-font);-webkit-font-smoothing:subpixel-antialiased;color:var(--black);display:flex;flex-direction:column}body blockquote{border-left:5px solid var(--cappuccino);color:var(--grey);font-size:1em}body .container{min-height:calc(100% - (header height + footer height));flex-grow:1}body .container .warning{margin:15px 0;background-color:var(--cream);border-left:5px solid var(--orange);padding-left:16px;color:var(--espresso)}body .container li{margin:4px 0}body .my-flexbox{display:flex}body .my-flexbox.center{align-items:center}body .my-flexbox.row-collapse{flex-direction:row}body .my-flexbox.column{flex-direction:column}body .my-flexbox.row{flex-direction:row}body .my-flexbox.row-reverse{flex-direction:row-reverse}body .my-flexbox.basic-gap{gap:6px}center.no-page{display:flex;flex-direction:column;height:100%;justify-content:center}header{background-color:var(--cream)!important}header .logo{margin-top:35px;width:279px}header .nav-wrapper{margin-top:56px;position:relative;margin-left:-10px}header a img:hover,header a img:focus{cursor:pointer!important}header svg{transform:rotate(180deg)}header nav.navbar-custom.invert .nav li a{font-family:var(--normal-font);color:var(--mocha);text-transform:capitalize;font-size:18px}header nav .container-fluid{padding:0}header .meta{font-size:13px;font-style:italic}header .tags{margin:20px 0 10px}h1,h2,h3,h4,h5,h6{font-family:var(--titles-font);color:var(--espresso)}.post-content-preview{font-family:var(--normal-font);font-size:14px;font-style:normal;color:var(--black)!important}.post-preview .post-meta{font-size:13px}.post-preview .post-cover{height:196px;width:196px;margin:5px;border-radius:3px}img.cover-img{width:196px;height:196px;border-radius:10px}.mini-post-list .post-preview{margin-left:0}img.cover-tags,.post-preview .cover-tags{width:100px;height:100px}.pager li>a,.pager li>span{border:1px solid #fff}.post-container a{color:var(--cappuccino)}.post-container img{display:block;max-width:unset;height:auto;margin:0}footer{margin-top:10px;background-color:var(--cream);color:var(--espresso);font-family:var(--titles-font);font-size:18px;padding:25px 0 5px}footer .list-inline{margin-bottom:15px}footer .logo{width:130px}footer p{text-align:center;margin:5px 0}footer .copyright{font-family:var(--normal-font)}footer .copyright a{color:var(--cappuccino)}